import java.util.HashSet;


public class Exemplo {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		HashSet<String> set = new HashSet<String>();
		set.add("maria");
		set.add("maria");
		set.add("maria");
		set.add("evandro");
		System.out.println(set.size());

	}

}
